   "source": [
    "from rdkit import Chem\n",
    "from rdkit.Chem import Draw\n",
    "\n",
    "# Chuỗi InChI của phân tử\n",
    "inchi = \"InChI=1S/C2H4O2/c1-2(3)4/h1H3,(H,3,4)\"\n",
    "\n",
    "# Tạo đối tượng Molecule từ chuỗi InChI\n",
    "mol = Chem.MolFromInchi(inchi)\n",
    "\n",
    "# Vẽ cấu trúc hoá học\n",
    "Draw.MolToImage(mol)"
   ]
